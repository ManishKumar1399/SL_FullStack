<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>JDBCSetup</servlet-name>
		<servlet-class>com.simplilearn.JDBCSetup</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProductDetails</servlet-name>
		<display-name>ProductDetails</display-name>
		<description></description>
		<servlet-class>com.simplilearn.ProductDetails</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StoredProcedures</servlet-name>
		<display-name>StoredProcedures</display-name>
		<description></description>
		<servlet-class>com.simplilearn.StoredProcedures</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DBOperations</servlet-name>
		<servlet-class>com.simplilearn.DBOperations</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StudentsDetails</servlet-name>
		<servlet-class>com.simplilearn.StudentsDetails</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JDBCSetup</servlet-name>
		<url-pattern>/JDBCSetup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProductDetails</servlet-name>
		<url-pattern>/products</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StoredProcedures</servlet-name>
		<url-pattern>/procedures</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DBOperations</servlet-name>
		<url-pattern>/operations</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentsDetails</servlet-name>
		<url-pattern>/students</url-pattern>
	</servlet-mapping>
</web-app>
